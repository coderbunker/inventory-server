<% if (HOWTO) { %>
  <article class="well col-xs-12">
    <h2>How To</h2>
    <%- HOWTO %>
  </article>
<% } %>
<% if (Troubleshooting) { %>
  <article class="well col-xs-12">
    <h2>Need Help</h2>
    <%- Troubleshooting %>
  </article>
<% } %>
<% if ( similarItems.length > 0 ) { %>
  <article class="well col-xs-12">
    <h2>Similar Items</h2>
    <% similarItems.forEach(function(item) { %>
      <%- include('./partials/itemCards', item); %>
    <% }) %>
  </article>
<% } %>
<article class="well col-xs-12">
  <h2>Details</h2>
  <%- details %>
  <ul>
    <li>model: <%- model %></li>
    <li>serial: <%- serial %></li>
    <li>owner: <%- owner %></li>
    <li>UUID: <%- uuid %></li>
  </ul>
</article>
